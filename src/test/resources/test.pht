(use-ctx pht
    (cls StringExtends [^Object]
        (@varargs (efn ^String sub ^String [[arr (array-type ^String)]] (progn
            (def [[str (new ^StringBuilder this)]])
            (for [e arr]
                (#append str e))
            (#toString str)))))
    (app-fn
        (println (- "Слава " "России!"))))